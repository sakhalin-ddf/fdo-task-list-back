{
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "exclude-from-classmap": [
            "src/Migrations"
        ]
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-curl": "*",
        "ext-fileinfo": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-zlib": "*",
        "doctrine/doctrine-bundle": "^2.5",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^2.11",
        "symfony/cache": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dotenv": "^6.0",
        "symfony/flex": "^2.0",
        "symfony/http-foundation": "^6.0",
        "symfony/symfony": "^6.0"
    },
    "extra": {
        "symfony": {
            "docker": false
        }
    },
    "require-dev": {
        "zircote/swagger-php": "^4.2",
        "symplify/easy-coding-standard": "^10.0",
        "symfony/web-profiler-bundle": "^6.0",
        "roave/security-advisories": "dev-latest"
    }
}
